{
  "name": "hacker-bench",
  "version": "1.0.0",
  "description": "An MVP electron-based mock of humanbenchmark.com",
  "main": "electron.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"webpack --watch\" \"electron .\"",
    "build": "rimraf ./public && mkdir public && webpack --progress -p"
  },
  "repository": "https://github.com/kyleperra/hackr-bench",
  "keywords": [
    "aimtrainer",
    "aim",
    "benchmark"
  ],
  "devDependencies": {
    "electron": "9.3.1",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12",
    "@babel/core": "7.12.9",
    "@babel/preset-env": "7.12.7",
    "@babel/preset-react": "7.12.7",
    "babel-loader": "8.2.1",
    "css-loader": "3.6.0",
    "file-loader": "6.2.0",
    "style-loader": "1.2.1",
    "url-loader": "4.1.0",
    "concurrently": "5.3.0",
    "rimraf": "3.0.2"
  },
  "dependencies": {
    "@nivo/core": "^0.67.0",
    "@nivo/line": "^0.67.0",
    "axios": "^0.21.0",
    "cors": "^2.8.5",
    "express": "4.17.1",
    "random-words": "^1.1.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ],
      "@babel/preset-react"
    ]
  }
}
